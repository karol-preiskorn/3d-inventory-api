{
  "destination": "./out/",
  "plugins": [],
  "recurse": true,
  "recurseDepth": 10,
  "source": {
    "include": [
      "src",
      "realm",
      "schema"
    ],
    "excludePattern": "(^|\\/|\\\\)_",
    "includePattern": ".+\\.(j|t)s(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
