{
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "source": {
    "excludePattern": "(node_modules|docs)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
