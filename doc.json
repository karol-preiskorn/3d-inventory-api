{
  "plugins": ["./plugins/markdown", "./plugins/summarize"],
  "recurseDepth": 10,
  "source": {
    "exclude": ["jest.config.js", "node_modules/", "build/"],
    "include": ["src/"]
  },
  "sourceType": "commonjs",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  }
}
