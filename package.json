{
  "name": "3d-inventory-mongo-api",
  "version": "0.1.22",
  "description": "Mongo API for 3d-inventory project",
  "keywords": [
    "3d-inventory",
    "mongo",
    "api"
  ],
  "homepage": "https://github.com/karol-preiskorn/3d-inventory-mongo-api#readme",
  "bugs": {
    "url": "https://github.com/karol-preiskorn/3d-inventory-mongo-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/karol-preiskorn/3d-inventory-mongo-api.git"
  },
  "license": "Common Development and Distribution License",
  "author": "Karol Preiskorn",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets='@babel/preset-env' src/index.js",
    "build": "babel src -d build && node build/index.js",
    "clean:cache": "npm cache clean --force",
    "clean:cleanup": "rm package-lock.json && rm -rf ./node_modules",
    "format": "prettier --single-quote --check .",
    "gts:browse": "gcloud app browse",
    "gts:clean": "gts clean",
    "gts:deploy": "gcloud app deploy",
    "gts:fix": "gts fix",
    "gts:gcp-build": "",
    "kill-port-usage": "kill -9 `lsof -nP -iTCP:8080 -sTCP:LISTEN | sed -n 2p | awk '{print $2}'`",
    "lint": "gts lint",
    "lint:docs": "jsdoc --readme README.md --destination doc *",
    "lint:eslint": "clear && echo esLint  `date +%R` | /usr/bin/figlet -f ./assets/font.flf && eslint \"**/*.js\" --fix",
    "lint:js": "eslint --ignore-path .gitignore --config .eslintrc  \"**/*.js\"",
    "maven:latest": "cd api && mvn versions:use-latest-versions",
    "maven:updates": "cd api && mvn versions:display-plugin-updates",
    "openapi:format": "npx openapi-format api/openapi.yaml --output api/openapi-formatted.yaml",
    "prepare": "node .husky/install.mjs",
    "prepare:husky": "cd ../ && husky install server/.husky",
    "start": "node --experimental-modules --trace-warnings build/index.js",
    "start:debug": "nodemon --exec npm run babel-node -- src/index.js",
    "swagger:editor": "podman run -d -p 80:8080 -v ${PWD}:/tmp -e SWAGGER_FILE=/tmp/api/openapi.yaml swaggerapi/swagger-editor",
    "swagger:generate": "pwd && java -jar api/target/jars/swagger-codegen-cli.jar generate -i api/openapi.yaml -l nodejs-server -o src",
    "swagger:get-jars": "mvn package -f api/pom.xml",
    "swagger:podman-generate": "podman run --rm -v ${PWD}:/local swaggerapi/swagger-codegen-cli-v3 generate -i /local/api/openapi.yaml -l nodejs-server -o /local/src",
    "test": "c8 mocha -p -j 2 tests/*.test.js --exit",
    "test:detectOpenHandles": "jest --detectOpenHandles",
    "test:jest": "node --trace-deprecation node_modules/jest/bin/jest.js ",
    "test:watch": "npm run test --watchAll",
    "unused": "./unused.sh",
    "update:update": "ncu -u && npm install",
    "update:upgrade": "npm update && npm upgrade"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.[m]js": [
      "npm --no-git-tag-version version patch",
      "prettier --double-quote --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@babel/register": "^7.23.7",
    "@google-cloud/storage": "^7.7.0",
    "async": "3.2.5",
    "babel-cli": "^6.26.0",
    "body-parser": "^1.20.2",
    "c8": "^9.1.0",
    "canvas": "^2.11.2",
    "cookie-session": "^2.1.0",
    "dotenv": "^16.4.5",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-jest": "^27.9.0",
    "express": "^4.18.3",
    "express-async-errors": "^3.1.1",
    "express-openapi-validator": "^5.1.6",
    "express-session": "1.18.0",
    "figlet": "^1.7.0",
    "fs": "^0.0.2",
    "google-auth-library": "^9.6.3",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "husky": "^9.0.11",
    "jest-changed-files": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-extended": "^4.0.2",
    "json-schema-to-openapi-schema": "^0.4.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.0.0",
    "moment": "^2.30.1",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.9",
    "npm-check-updates": "^16.14.15",
    "nyc": "^15.1.0",
    "oas-normalize": "^11.0.1",
    "oas3-tools": "^2.2.3",
    "open": "^10.0.3",
    "openapi-format": "^1.16.0",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "perlin-noise": "^0.0.1",
    "pretty-format": "^29.7.0",
    "regenerator-runtime": "^0.14.1",
    "rotating-file-stream": "^3.2.1",
    "semver": "^7.6.0",
    "server-destroy": "^1.0.1",
    "smoothstep": "^1.0.1",
    "swagger-ui-express": "^5.0.0",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^8.4.1",
    "@shelf/jest-mongodb": "^4.2.0",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "^48.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-runner-eslint": "^2.1.2",
    "jest-runner-prettier": "^1.0.0",
    "jsdoc": "^4.0.2",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "typedoc": "^0.25.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
