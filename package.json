{
	"name": "3d-inventory-mongo-api",
	"version": "0.1.1",
	"description": "Mongo API for 3d-inventory project",
	"keywords": [
		"3d-inventory",
		"mongo",
		"api"
	],
	"homepage": "https://github.com/karol-preiskorn/3d-inventory-mongo-api#readme",
	"bugs": {
		"url": "https://github.com/karol-preiskorn/3d-inventory-mongo-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/karol-preiskorn/3d-inventory-mongo-api.git"
	},
	"license": "Common Development and Distribution License",
	"author": "Karol Preiskorn",
	"main": "index.mjs",
	"scripts": {
		"cleanup": "rm package-lock.json && rm -rf ./node_modules",
		"format": "prettier --single-quote --check .",
		"kill-port-usage": "kill -9 `lsof -nP -iTCP:8080 -sTCP:LISTEN | sed -n 2p | awk '{print $2}'`",
		"lint:eslint": "echo EsLint - `date +%R` | /usr/bin/figlet && eslint \"*.mjs\" --quiet --fix",
		"lint:docs": "typedoc --out doc --tsconfig tsconfig.json src/** test/**",
		"lint:js": "eslint --ignore-path .gitignore --config .eslintrc src/**/*.mjs",
		"maven:latest": "cd api && mvn versions:use-latest-versions",
		"maven:updates": "cd api && mvn versions:display-plugin-updates",
		"openapi:format": "npx openapi-format api/openapi.yaml --output api/openapi-formatted.yaml",
		"prepare:husky": "cd ../ && husky install server/.husky",
		"start": "node --trace-warnings index.mjs",
		"start:debug": "nodemon --inspect --experimental-modules --verbose --delay 3 index.mjs",
		"start:install": "npm install",
		"swagger:editor": "podman run -d -p 80:8080 -v ${PWD}:/tmp -e SWAGGER_FILE=/tmp/api/openapi.yaml swaggerapi/swagger-editor",
		"swagger:generate": "pwd && java -jar api/target/jars/swagger-codegen-cli.jar generate -i api/openapi.yaml -l nodejs-server -o src",
		"swagger:get-jars": "mvn package -f api/pom.xml",
		"swagger:podman-generate": "podman run --rm -v ${PWD}:/local swaggerapi/swagger-codegen-cli-v3 generate -i /local/api/openapi.yaml -l nodejs-server -o /local/src",
		"test:jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js ",
		"test:detectOpenHandles": "jest --detectOpenHandles",
		"test:watch": "npm run test --watchAll",
		"update": "ncu -u && npm update && npm upgrade",
		"prepare": "npm run gcp-build",
		"pretest": "npm run gcp-build",
		"test": "c8 mocha -p -j 2 test/*.test.js --exit",
		"lint": "gts lint",
		"deploy": "gcloud app deploy",
		"browse": "gcloud app browse",
		"clean": "gts clean",
		"fix": "gts fix",
		"gcp-build": ""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.[m]js": [
			"prettier --double-quote --write",
			"eslint --fix"
		]
	},
	"dependencies": {
		"@apollo/client": "^3.8.9",
		"@babel/core": "^7.23.7",
		"@babel/preset-env": "^7.23.8",
		"@google-cloud/storage": "^7.7.0",
		"async": "3.2.5",
		"babel-cli": "^6.26.0",
		"babel-jest": "^29.7.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.2",
		"c8": "^9.1.0",
		"cookie-session": "^2.0.0",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"errorhandler": "1.5.1",
		"eslint-plugin-jest": "^27.6.3",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"express-flash": "0.0.2",
		"express-openapi-validator": "^5.1.2",
		"express-session": "1.17.3",
		"express-validator": "7.0.1",
		"figlet": "^1.7.0",
		"fs": "^0.0.1-security",
		"fs-extra": "^11.2.0",
		"google-auth-library": "^9.4.2",
		"graphql": "^16.8.1",
		"helmet": "^7.1.0",
		"http-errors": "^2.0.0",
		"husky": "^8.0.3",
		"jade-bootstrap": "^1.0.14",
		"json-schema-to-openapi-schema": "^0.4.0",
		"jsonwebtoken": "^9.0.2",
		"lodash": "^4.17.21",
		"lusca": "1.7.0",
		"markdown-it": "^14.0.0",
		"marked": "^11.1.1",
		"moment": "^2.30.1",
		"mongodb": "^6.3.0",
		"morgan": "^1.10.0",
		"morgan-body": "^2.6.9",
		"npm-check": "^3.2.10",
		"npm-check-updates": "^16.14.12",
		"oas-normalize": "^11.0.1",
		"oas3-tools": "^1.0.1",
		"openapi-format": "^1.15.4",
		"opn": "^6.0.0",
		"parent-module": "^3.1.0",
		"passport": "0.7.0",
		"passport-local": "1.0.0",
		"prettier": "^3.2.1",
		"pretty-format": "^29.7.0",
		"pug": "3.0.2",
		"punycode": "^2.3.1",
		"realm": "^12.5.1",
		"remove": "^0.1.5",
		"rotating-file-stream": "^3.2.1",
		"semver": "^7.5.4",
		"server-destroy": "^1.0.1",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.0",
		"winston": "3.11.0",
		"winston-daily-rotate-file": "^4.7.1",
		"yaml": "^2.3.4"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.3.1",
		"@shelf/jest-mongodb": "^4.2.0",
		"chai": "^5.0.0",
		"chai-http": "^4.4.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-config-standard": "17.1.0",
		"eslint-plugin-import": "2.29.1",
		"jest": "^29.7.0",
		"mocha": "^10.2.0",
		"nodemon": "^3.0.2",
		"supertest": "^6.3.3",
		"typedoc": "^0.25.7",
		"wait-port": "1.1.0"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
